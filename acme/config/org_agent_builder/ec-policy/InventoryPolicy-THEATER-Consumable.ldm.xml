<?xml version="1.0"  encoding="US-ASCII"?>
<!DOCTYPE Policy SYSTEM "Policy.ldm.dtd" []>
<Policies>
<!-- Inventory Policy for Consumable at THEATERechelon of support -->
 <Policy name = "SparePartsPolicy" type="org.cougaar.logistics.plugin.inventory.InventoryPolicy">
    <RuleParam name="TransportTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="ResourceType">
        <String value = "Consumable">
        </String>
    </RuleParam>
    <RuleParam name="OrderShipTime">
        <Integer value = "20" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="SupplierAdvanceNoticeTime">
        <Integer value = "60" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="HandlingTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="CriticalLevel">
        <Integer value = "20" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="ReorderPeriod">
        <Integer value = "10" min="0" max="1000">
        </Integer>
    </RuleParam>
 </Policy>
</Policies>
