<?xml version="1.0"  encoding="US-ASCII"?>
<!DOCTYPE Policy SYSTEM "Policy.ldm.dtd" []>
<Policies>
<!-- Inventory Policy for Ammunition at CONSUMERechelon of support -->
 <Policy name = "AmmunitionPolicy" type="org.cougaar.logistics.plugin.inventory.InventoryPolicy">
    <RuleParam name="TransportTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="ResourceType">
        <String value = "Ammunition">
        </String>
    </RuleParam>
    <RuleParam name="OrderShipTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="SupplierAdvanceNoticeTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="HandlingTime">
        <Integer value = "1" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="CriticalLevel">
        <Integer value = "3" min="0" max="1000">
        </Integer>
    </RuleParam>
    <RuleParam name="ReorderPeriod">
        <Integer value = "3" min="0" max="1000">
        </Integer>
    </RuleParam>
 </Policy>
</Policies>
